<li class="span4">
    <a href="/#mission/<%= id %>">
        <div class="thumbnail mission-box">
            <img alt="300x200" src="<%= (typeof(cover.photo) !== "undefined") ? cover.photo.url : null %>" style="width: 300px; height: 200px;">
            <div class="caption">
                <h4 class="mission-title"><%= name %></h4>
                <p class="mission-price">от 68 000 руб.</p>
                <div class="clearfix"></div>
            </div>
        </div>
    </a>
</li>